# Subsite configuration
# ---------------------

project.id = cwt
project.install.modules = cwt_core
project.name = Commissioners
project.theme.default = europa
project.url.production = https://ec.europa.eu/commission/commissioners

# Development modules to enable
# -----------------------------
devel.mdls.en = devel field_ui maillog views_ui masquerade log_filter module_filter judy imagecache_defaults hotjar dt_dev security_review dt_acceptance dt_flexible_date dt_test_flexible_date administration_language_negotiation

# Solr configuration.
# -------------------
solr.enable = 0

# Platform configuration.
# -----------------------
profile = multisite_drupal_standard
platform.package.version = 2.4

# Behat configuration.
# --------------------
behat.test_tags = @cwt
behat.yml.path = ${behat.dir}/behat.cwt.yml
behat.show_cmd = open %s
